@model Blog.web.Models.ViewModels.ListBlogRequest
@{
}

<div class="bg-secondary bg-opacity-10 py-2">
    <div>
        <h1>All Blogposts - Admin Functionality</h1>
    </div>
</div>

<div class="container py-5">
    <table class="table"> 
        <thead>
            <tr>
                <td>Id</td>
                <td>Heading</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var blogpost in Model.listOfBlogs)
            {
                <tr>
                    <td>@blogpost.Id</td>
                    <td>@blogpost.Heading</td>
                </tr>
            }
        </tbody>
    </table>
</div>